#include <bits/stdc++.h>
using namespace std;
int lucky[] = {4, 7, 44, 47, 74, 77, 444, 777, 474, 747, 447, 774, 744, 477};
int main() {
    
    int n;
    scanf("%d", &n);
    for(int i = 0; i < 14; ++i)
        if( n % lucky[i] == 0 || n == lucky[i]){
            puts("YES");
            return 0;
        }
    puts("NO");    
   
    return 0;
}
