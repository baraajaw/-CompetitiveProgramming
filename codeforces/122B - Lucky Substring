#include<bits/stdc++.h>
using namespace std; 

int main(){


  string s;
  int s4 = 0 , s7 = 0;
  cin >> s;
  
  for ( int i = 0 ; i < s.size() ; ++i){
    if ( s[i] == '4')
     s4++;
    else if ( s[i]  == '7')
     s7++;
  }
  
  if ( s4 == 0 && s7 == 0 )
    puts("-1");
  else if ( s4 >= s7)
     puts("4");
  else if ( s4 < s7)
     puts("7");


  return 0;
}
