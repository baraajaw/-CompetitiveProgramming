#include <iostream>
#include <string>
#include <vector>
#include <stdio.h>
#include <cmath>
#include <algorithm>
#include <bitset>
#include <map>
#include <memory.h>
using namespace std;
int main(){
//	freopen ( "in" , "r" , stdin);

	 
   int n , m , l , r ;
   char c1 , c2;
   string s;
   scanf("%d%d" , &n , &m);

   cin >> s;

   for ( int i = 0 ; i < m ; ++i){
   	   scanf ("%d%d %c %c" , &l , &r , &c1 , &c2);
   	   for ( int j = l - 1; j <= r - 1; ++j)
   	   	  if ( s[j] == c1){
   	   	  	  s[j] = c2;
   	   	  }
   }
   cout << s << endl;

	return 0;
	}
