#include<iostream>
#include<string>
#include<cctype>
using namespace std;
int main()
{
	
	string s , t;
	bool flag = false;
	cin >> s >> t;
	int len = s.size();
	for ( int i = 0 ; i < len ; i++)
	{    
		if (tolower(s[i]) != tolower(t[i]))
		{
			if ( tolower(s[i]) <  tolower(t[i]))
				{
				cout << -1 <<endl;
			    return 0;
				}
			    else
			    {
			    	cout<< 1 <<endl;
			    	return 0;
			    }
		}
		
	
		
	}
		cout<< 0<<endl;
	
	return 0;
}
