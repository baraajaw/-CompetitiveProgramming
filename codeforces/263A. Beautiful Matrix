#include <bits/stdc++.h>
using namespace std;
int main() {
    
    int g[5][5], ans = 0;
    for (int i = 0; i < 5; ++i)
        for (int j = 0; j < 5; ++j){
            scanf("%d", &g[i][j]);
            if (g[i][j] == 1)
                ans = abs(2 - i) + abs(2 - j);
        }
    printf("%d\n", ans);    
   
    return 0;
}
