#include <bits/stdc++.h>
using namespace std;
int const N = 1e5 + 10;
int main() {
    int n;
    char s[110];
    string tmp = "hello";

    int j = 0;
    scanf("%s", s);     n = strlen(s);
    for(int i = 0; i < n; ++i){
        if(s[i] == tmp[j])
            j++;
        if(j == tmp.size()){
            puts("YES");
            return 0;
        }
    }
    puts("NO");

    return 0;
}
