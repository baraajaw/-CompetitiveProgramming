#include <bits/stdc++.h>
using namespace std;
int main() {
    
    int n, a[101], ans[101];
    scanf("%d", &n);
    for (int i = 0; i < n; ++i){
        scanf("%d", a + i);
        ans[a[i] - 1] = i + 1;
    }
    for (int i = 0; i < n; ++i)
        printf("%d ", ans[i]);
    puts("");
    
   
    return 0;
}
