#include<iostream>
#include<string>
#include<map>

using namespace std;
int main()
{
   int x , y;
   string s;
   map < string , int > mp;
    map < string , int > :: iterator it ;
  cin >> x >> y;

   for ( int i = 0 ; i < x ; i++)
     {
       cin >> s;
       for ( int j = 0 ; j < 3 ; j++)
       {
         mp[s.substr ( 0 , j + 1)]++;
         //cout << s.substr ( 0 , j + 1)<<endl;
       }
         
     }

  for ( int i = 0 ; i < y ; i++)
  {
     cin >> s ;
     cout<< mp[s]<<endl;
  }
  
  
  
  
  
  return 0;
}
