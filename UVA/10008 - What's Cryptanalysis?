#include <iostream>
#include <string>
#include <cctype>
#include<algorithm>
using namespace std;

int main() {

int x , c = 0 , q;
pair < int , int > p[26];
string s , st = "";

for ( int i  = 0 ; i < 26 ; i++ )
{
     p[i].first= 0;  
     p[i].second= i;  

}
//**************************
cin >> x;
cin.ignore();
//************************
for ( int i = 0 ; i < x ; i++)
{
  getline ( cin , s);
  for ( int j = 0 ; j < s.size() ; j++)
      p[ toupper(s[j]) - 'A'].first++;
}
//***********************
sort ( p , p + 26);
int w =  25;
while ( w > 0)
  {
    if ( p[w].first == p[w - 1].first)
       {
         c++;
         st += char ( p[w].second + 'A' );
         q = p[w].first;
       }
       else if ( p[w].first != p[w - 1].first && c > 0)
       {
         st += char ( p[w].second + 'A' );
         c++; 
        // cout<<st<<endl;
         for (int j = st.size() - 1 ; j >= 0 ; j--)
          cout<<st[j]<<" "<<q<<endl;
          //w -= c;
          c = 0;
          st = "";
          
        }
        else if ( p[w].first != 0)
        {
          cout<<char ( p[w].second + 'A')<<" "<<p[w].first<<endl;
        }
        w--;
    
  }
  


return 0;
  
}
