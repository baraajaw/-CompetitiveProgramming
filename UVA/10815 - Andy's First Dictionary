#include <iostream>
#include <stdio.h>
#include <string>
#include <string.h>
#include <vector>
#include <algorithm>
#include <memory.h>
#include <map>
#include <queue>
#include <stack>
#include <set>
#include <cmath>
#include <time.h>
#include <bitset>
#define ll long long 
#define f(i, x, n) for(int i = x; i < (int)(n); ++i)
using namespace std;
int const N = 1e7 + 1;
int main() {
  char s[5006];
  set < string > st;
  string c;
  while (gets(s)){
      int j = 0;
      string tmp ="";
      c = s;
      f(i, 0, c.size()){
        if ( isalpha (c[i])) tmp += tolower(c[i]);
        else if ( tmp != ""){
          st.insert(tmp);
          tmp = "";
        }
      }
      if ( tmp.size() > 0) st.insert(tmp);
  }
  for ( auto it = st.begin() ; it != st.end() ; it++)
    cout<< * it << endl;
  return 0;
}
