#include<iostream>
#include<string>
#include<map>
using namespace std; 
int main()
{
  int x; 
  map < string , int> m;
  string s , st;
  cin>>x;
  cin.ignore();
  for ( int i = 0 ; i < x ; i++)
  {
   getline(cin , s);
    m[s.substr( 0 , s.find(' '))]++;
  }
  
   map < string , int> ::iterator it;
  
   for ( it = m.begin(); it != m.end() ; it++)
   {
     cout<<it->first<<" "<<it->second<<endl;
   }
  
  
  
  return 0;
}
