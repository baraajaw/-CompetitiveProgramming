#include <iostream>
#include<map>
#include<string>
using namespace std;

int main() {

map <string , int > arr ;
int x , y , w , t = 0;

string s , c = "";
cin >> x >> y;
 map <string , int > :: iterator it;
 for (int i = 0 ; i < x ; i++)
      {
        cin >> s >> w;
        arr[s] = w;
      }
  
  it = arr.begin();
  for ( int i = 0 ; i < y ; i++)
  {
    while( getline ( cin , s ) && s != ".")
       {  s = s +" ";
         for ( int j = 0 ; j < s.size() ; j++)
              {
                if (s[j] != ' ')
                c += s[j];
                else
                {
                 t += arr[c];
                 
                 c = "";
                }
              }
              //cout<<endl;
        }
        cout<<t<<endl;
        t = 0;
  }



return 0;
  
}
